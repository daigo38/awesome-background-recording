{
  "expo": {
    "name": "SampleRecordExpo",
    "slug": "sample-record-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.sampleRecordExpo",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSMicrophoneUsageDescription": "This app records audio in the background."
      },
      "appleTeamId": "8MU425T8JV"
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro"
    }
  }
}
